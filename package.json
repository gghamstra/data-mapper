{
  "name": "drawflow-theme-generator",
  "version": "1.0.0",
  "description": "A comprehensive theme generator for Drawflow with TypeScript support",
  "main": "dist/theme-generator-main.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "serve": "npx http-server . -p 8080 -o",
    "start": "npm run build && npm run serve"
  },
  "keywords": [
    "drawflow",
    "theme-generator",
    "typescript",
    "flow-editor",
    "visual-programming"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "drawflow": "^0.0.60"
  }
}
